<!DOCTYPE html>
<html>
  <head>
    <title>MessageMe</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
  
      <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
       <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    
  </head>

  <body>

  <%#NAVBAR%>

  <div class="ui inverted small menu">
  <div class="ui container">
    <a class="active item">
      Home
    </a>
    <a class="item">
      Messages
    </a>
    <div class="right menu">
      <div class="ui dropdown simple item">
        User <i class="dropdown icon"></i>
        <div class="menu">
          <a class="item">Edit</a>
          <a class="item">Info</a>
          <a class="item">History</a>
        </div>
      </div>
      <div class="item">
          <div class="ui primary button">Sign Up</div>
      </div>
    </div>
</div>
</div>

<div class="ui container">
    <%= yield %>
<div>
  </body>
</html>
